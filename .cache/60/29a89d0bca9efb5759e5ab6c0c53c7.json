  save(): void {\r\n    this.sync\r\n      .save(this.attributes.getAll())\r\n      .then((res: AxiosResponse): void => {\r\n        this.trigger(\"save\");\r\n      })\r\n      .catch(() => {\r\n        this.trigger(\"error\");\r\n      });\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"a9c9a8a6d2f719a06b3c45059499779a","cacheData":{"env":{}}}